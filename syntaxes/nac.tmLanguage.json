{
  "scopeName": "source.nac",
  "name": "NaC",
  "patterns": [
    {
      "name": "comment.line.double-slash.nac",
      "match": "//.*$"
    },
    {
      "name": "keyword.control.nac",
      "match": "\\b(fn|rn|if|for|while|in|out|time|break|continue|array|http)\\b"
    },
    {
      "name": "support.function.builtin.nac",
      "match": "\\b(sqrt|pow|sin|cos|tan|abs|floor|ceil|round|log|exp|length|upper|lower|push|pop|trim|replace|substr|indexOf|first|last|reverse|slice|join|read|write|append)\\b"
    },
    {
      "name": "constant.numeric.nac",
      "match": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "string.quoted.double.nac",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.nac",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "variable.other.nac",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
    },
    {
      "name": "keyword.operator.nac",
      "match": "\\+\\+|--|==|!=|<=|>=|&&|\\|\\||[+\\-*/%=<>!]"
    },
    {
      "name": "punctuation.terminator.nac",
      "match": "[;:,\\[\\]\\(\\){}]"
    }
  ]
}